generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Individual {
  id        String   @id @default(uuid())
  name      String
  gender    String?
  birthdate DateTime?
  fatherID  String?
  motherID  String?
  partnersID String[]
  profile   String?
}